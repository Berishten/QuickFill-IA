{
  "manifest_version": 3,
  "name": "Quick fill IA",
  "version": "1.0",
  "description": "Fill out web forms in seconds using personalized contexts",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "./js/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./js/content.js"
      ]
    }
  ]
}