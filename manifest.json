{
  "manifest_version": 3,
  "name": "QuickMe",
  "version": "1.0",
  "description": "QuickMe is a simple extension that allows you to quickly answer form inputs based on a file.",
  "permissions": [
    "activeTab",
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}